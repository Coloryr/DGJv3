<Window x:Class="ExtendNetease_DGJModule.CookieLoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExtendNetease_DGJModule"
        mc:Ignorable="d"
        Title="Cookie登录 - 管理界面 - 本地网易云喵块" SizeToContent="WidthAndHeight" WindowStyle="ToolWindow" ResizeMode="CanMinimize" Width="500">
    <StackPanel>
        <DockPanel Margin="5,5,5,5">
            <TextBlock Text="Cookie:"/>
            <TextBox x:Name="CookieBox" Margin="5,0,0,0"/>
        </DockPanel>
        <Button x:Name="SubmitBtn" Content="确认" Margin="5,5,5,5" Click="Submit_Click"/>
    </StackPanel>
</Window>
